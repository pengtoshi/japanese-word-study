# Supabase (client + server auth via cookies)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# OpenAI (later)
OPENAI_API_KEY=

# OpenAI model routing (optional)
# Recommended for this app:
# - Generate: gpt-5-mini
# - Grade: gpt-5-nano (fallback to gpt-5-mini)
# - Autofill: gpt-5-nano (fallback to gpt-5-mini)
OPENAI_MODEL_GENERATE=
OPENAI_MODEL_GENERATE_FALLBACK=
OPENAI_MODEL_GRADE=
OPENAI_MODEL_GRADE_FALLBACK=
OPENAI_MODEL_AUTOFILL=
OPENAI_MODEL_AUTOFILL_FALLBACK=

# Debug: include raw OpenAI response in API error messages (be careful in production)
OPENAI_DEBUG_RAW_RESPONSE=false
OPENAI_GRADE_FALLBACK_ON_NEEDS_FIX=true


